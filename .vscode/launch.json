{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Attach to Chrome",
			"port": 9222,
			"request": "attach",
			"type": "chrome",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Debug server",
			"port": 9229,
			"skipFiles": ["<node_internals>/**"]
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Debug client",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}"
		}
	]
}
